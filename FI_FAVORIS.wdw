  procedures :
   -
     name : ActualiseFavoris
     procedure_id : 2280288872928010373
     type_code : 14
     code : |1-
      // Résumé : Actualise les favoris
      // Syntaxe :
      // ActualiseFavoris ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure ActualiseFavoris()
      
      	mafenetre..AffichageActif = faux
      
      	// Vide la liste
      	Zr_FAVORI.supprimetout()
      
      	// Pour chaque favori
      	POUR TOUT stFavori de Favoris
      		
      		// Ajoute une ligne
      		soit N = ZR_FAVORI.AjouteLigne()
      		
      		// Renseigne les informations
      		ZR_FAVORI[ N ].LIB_TITRE_FAVORI	= stFavori.Nom
      		ZR_FAVORI[ N ].LIB_URL			= stFavori.URL
      		ZR_FAVORI[ N ].IMG_FAVICON		= DataURIVersImage( stFavori.Icône )
      		
      	FIN
      	
      	MaFenêtre..AffichageActif = vrai
      	
      	LIB_VIDE.Visible			= ( Favoris.Vide )
      	OPT_TOUT_SUPPRIMER.Grisé	= ( Favoris.Vide )
      	OPT_EXPORTER_FAVORIS.Grisé	= ( Favoris.Vide )
     type : 458752
