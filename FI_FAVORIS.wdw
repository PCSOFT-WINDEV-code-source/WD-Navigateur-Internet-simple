    internal_properties : 
    options :
     -
       name : OPT_IMPORTER_FAVORIS
       identifier : 0x2fa5361203901cde
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           internal_properties : CAAAAAgAAACxKHcdzAW2fL1J6TsagXoaS4NWFT/heaopfqCJiRcY7AEbY2i8sI4klNoUEHPtNckrbBzdjM94WzPe3jJ55mz+6kxiXocu42ZKs/teCAiTmVcEkR9yxvu6bhsFXzbi4bERE7JtHHx3PP5gEF7wdf+5jjyaNkkIjen0wReeukFuYPtb6x4vQ+TZMixXAZzzXp0=
           code : |1-
            
            sFichier est une chaine = fselecteur( "", "", "Ouvrir", "Documents HTML (*.HTML)" + tab + "*.HTM;*.HTML", "*.HTML", fselOuvre )
            si sFichier <> "" ALORS
            	
            	// On charge les documents de type Netscape-Bookmark-File
            	
            	// Ouvre le document 
            	htmlFavoris est un htmlDocument = htmlouvre( sFichier, depuisFichier )
            	
            	HTMLParcoursFils( htmlFavoris.html.body )
            	procédure interne HTMLParcoursFils( htmlRacine est un htmlnoeud )
            	
            		// Enumère les noeuds fils
            		pour tout htmlNoeud de htmlRacine..NoeudFils
            			
            			// Ce qui nous intéresse dans le document, ce sont les balises  <A> uniquement 
            			si htmlNoeud..NomBalise ~= "A" ALORS
            
            				stEntrée est une EntréeFavori
            				
            				quand exception dans
            				
            					stEntrée.Date  = EpochVersDateHeure( htmlNoeud..Attribut[ "add_date" ]..Valeur )
            					stEntrée.Nom   = htmlNoeud..Texte
            					stEntrée.URL   = htmlNoeud..Attribut[ "href" ]..Valeur
            					stEntrée.Icône = htmlNoeud..Attribut[ "icon" ]..Valeur
            					
            				faire
            					
            					VariableRAZ( stEntrée )
            					
            				fin
            								
            				// On vérifie si on a déjà une entrée identique
            				si stEntrée.URL <> "" _et_ FavoriExiste( stEntrée.url ) = faux ALORS
            													
            					// Ajoute l'entrée
            					Favoris.Ajoute( stEntrée )	
            				FIN
            			sinon
            				HTMLParcoursFils( htmlNoeud )
            			FIN			
            		FIN
            	fin
            	
            	// Sauvegarde
            	SauveFavoris()
            	
            	// Réinitialise la liste et l'affichage
            	ActualiseFavoris()
            FIN
           type : 32
     -
       name : OPT_EXPORTER_FAVORIS
       identifier : 0x2fa53628039774c2
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           internal_properties : CAAAAAgAAADt5CzvqduMzYuUQb/YRFL1ijRWXC/4Nj0fcFqEANdLD2ht0eHWzVtvLGdkPX/C8VaXu2HPssM4U6pDM4wFYd8uMp6YUgkuwOqag00cVGTnrghFbJDSLjDXUoOuGj4oI/XaBlH5q56eWa9hLKc66yltyIcNg6Xo7WE0CS92UUdi4Q==
           code : |1+
            
            // Sauvegarde les favoris au format standard NETSCAPE-Bookmark-file
            
            Epoch est un entier sans signe = DateHeureVersEpoch( Maintenant, epochUnixSeconde )
            sNomGénéré est une chaine = "favoris_" + dateVersChaine( datedujour, "JJ_MM_AAAA" ) + ".html"
            sFichier est une chaine
            
            // Ouvre le sélecteur de fichiers
            sFichier = fSélecteur( "", sNomGénéré, "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Documents HTML (*.HTML)" + TAB + "*.HTM;*.HTML", "*.HTM;*.HTML", fselCrée + fselExiste )
            si sFichier <> "" ALORS
            	
            	// Entête du fichier
            	sHTML est une chaine = [
            	<!DOCTYPE NETSCAPE-Bookmark-file-1>
            	<!-- This is an automatically generated file.
            		It will be read and overwritten.
            		DO NOT EDIT! -->
            	<META HTTP-EQUIV="Content-Type" CONTENT="text/html; charset=UTF-8">
            	<TITLE>Bookmarks</TITLE>
            	<H1>Bookmarks</H1>
            	<DL><p>
            		<DT><H3 ADD_DATE="[%Epoch%]" LAST_MODIFIED="[%Epoch%]" PERSONAL_TOOLBAR_FOLDER="true">Barre des favoris</H3>
            		<DL><p>
            	
            	]
            	
            	// Génère les entrées 
            	stFavori est une EntréeFavori
            	pour tout stFavori de Favoris
            		
            		sHTML += [
            			<DT><A HREF="[%stFavori.url%]" ADD_DATE="[%stFavori.Date.VersEpoch( epochUnixSeconde )%]" ICON="[%stFavori.Icône%]">[%stFavori.nom%]</A>
            		
            		]
            		
            	FIN
            	
            	sHTML += [
            	</DL><p>
            	]
            	
            	// Sauvegarde
            	fSauveTexte( sFichier, sHTML )
            FIN
           type : 32
