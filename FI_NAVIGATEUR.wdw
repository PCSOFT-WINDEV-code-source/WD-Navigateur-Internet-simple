     name : OuvreNavigateur
     internal_properties : CAAAAAgAAABclsdrTxb6qD06YRxt7pPgcMLE78sabyIGYza8E4mNHhSpuPnDAHPFBN5hSH/vWGk6bZyVmp2LnBRbx5u93epsZSoLxRBv9TfKV9fGSPhAUdcrA1ZxtSnqPk1na4sfa6akGDRRLXY53vM9JTM90OgE7HPw0qkIkMoIJuw9hnVKWZ4ABzyt7n9Y7P8Xf66gO4s+inaxMAvy6cdrAnhxvFTSj1yZpsLszrmFhtI1eYFfhlQE7ntblSjiywEzgyc=
     procedure_id : 2280283684607423151
     type_code : 14
     code : |1-
      // Résumé : Ouvre une URL avec un nouvel onglet
      // Syntaxe :
      //OuvreNavigateur (<sUrl> est chaîne [, <sAlias> est chaîne [, <bConserveVoletCourant> est booléen [, <bDétacheOnglet> est booléen]]])
      //
      // Paramètres :
      //	sUrl (chaîne ANSI) : Url à ouvrir
      //	sAlias (chaîne ANSI - valeur par défaut="") : Alias de ce navigateur
      //	bConserveVoletCourant (booléen - valeur par défaut=1) : Vrai si on doit conserver le volet actuellement affiché en premier plan, Faux sinon
      //	bDétacheOnglet (booléen - valeur par défaut=0) : Vrai pour détacher le nouvel onglet, Faux sinon
      // Valeur de retour :
      // 	Aucune
      //
      procédure OuvreNavigateur( sUrl est une chaine, sAlias est une chaine = "", local bConserveVoletCourant est un booléen = vrai, bDétacheOnglet est un booléen = faux ) 
      
      	sAliasOuvert est une chaine
      
      	SI bDétacheOnglet ALORS
      		
      		// Ouvre une nouvelle fenêtre
      		fen_main.OuvreSoeur( sUrl )
      		bConserveVoletCourant = faux
      		retour
      	FIN
      
      	mafenetre.AffichageActif = faux
      
      	// Récupère le champ onglet du volet
      	oOnglet est un champ <- { OngletDuChamp( mafenetreinterne ), indchamp }
      	
      	// Si on a fourni un alias
      	si sAlias <> "" ALORS
      		
      		// Vérifie si on n'a pas déjà un volet ouvert avec cet alias
      		
      		// Enumère les volets de l'onglet
      		soit N = 1 
      		soit Alias = EnumèreChamp( oOnglet, N )
      		
      		TANTQUE Alias <> ""
      			
      			// Si l'alias correspond
      			si Alias ~= sAlias ALORS
      				
      				// Prend une référence sur le navigateur ouvert dans le volet
      				oNavigateur est un Champ <- oOnglet[ Alias ].FI_NAVIGATEUR
      				
      				// Demande au volet d'afficher l'url
      				oNavigateur.URL = sUrl
      								
      				SI bConserveVoletCourant = faux ALORS
      					
      					// Force le volet à être affiché
      					oOnglet = Alias
      					sAliasOuvert = Alias
      				FIN
      				
      				retour
      			FIN
      			
      			// Volet suivant
      			N ++
      			Alias = EnumèreChamp( oOnglet, N )
      		FIN
      	FIN
      		
      	// Ouvre un nouvel onglet
      	sAliasOuvert = oOnglet.Ouvre( ( sUrl = "" ? "Nouvel onglet" sinon "Chargement..." ), FI_NAVIGATEUR, sUrl, sAlias )
      	
      fin:
      	
      	si bConserveVoletCourant alors
      		
      		// Restaure le volet affiché
      		oOnglet = __volet.alias
      		sAliasOuvert = __volet.alias		
      	
      	fin
      	
      	
      
      	mafenetre.AffichageActif = vrai
     type : 458752
