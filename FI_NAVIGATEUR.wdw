            PROCEDURE ClicDroitLien( local sUrl est chaine )
            
            // Redéfinit les actions des options de menus
            
            
            // - Ouvrir le lien dans un nouvel onglet
            MENU_LIEN.OPT_LIEN_NOUVEL_ONGLET..Traitement[ trtOptionMenu ] = () => {
            
            	// On ouvre l'URL dans un nouveau navigateur
            	OuvreNavigateur( sUrl, "" )
            }
            
            // - Ouvrir le lien dans une nouvelle fenêtre
            MENU_LIEN.OPT_LIEN_NOUVELLE_FENETRE..Traitement[ trtOptionMenu ] = () => {
            
            	// On ouvre l'URL dans un nouveau navigateur
            	OuvreNavigateur( sUrl, "", Vrai, vrai )
            	
            }
            
            // - Enregistrer le lien sous
            MENU_LIEN.OPT_LIEN_ENREGISTRER_SOUS..Traitement[ trtOptionMenu ] = () => {
            
            	sNomFichier est une chaine = URLExtraitChemin( sUrl, urlNomRessource + urlExtensionRessource )
            	si sNomFichier = "" ALORS
            		sNomFichier = "téléchargement.html"
            	FIN
            
            	// Ouvre le sélecteur de fichier
            	sFichier est une chaine = fselecteur( "", sNomFichier, "Enregistrer sous", "Tous les fichiers (*.*)" + tab + "*.*", "*.*", fselCrée + fselExiste )
            	si sFichier = "" alors retour
            	
            	// Prépare le téléchargement
            	stTéléchargement est une httpRequête
            	stTéléchargement.Url = sUrl
            	stTéléchargement.Destination = sFichier
            	
            	// Télécharge le fichier
            	stTéléchargement.Envoie()
            
            	// On prévient
            	ToastAffiche( "Téléchargement terminé", toastCourt, cvMilieu, chCentre )
            }
            
            // - Copier le lien
            MENU_LIEN.OPT_LIEN_COPIER..Traitement[ trtOptionMenu ] = () => {
            
            	// Copie l'url dans le presse-papier
            	VersPressePapier( sUrl ) 
            	
            	// On prévient
            	ToastAffiche( "Lien copié", toastCourt, cvMilieu, chCentre )
            }
            
            OuvreMenuContextuel( MENU_LIEN )
