            PROCÉDURE ClicLien( sUrl est chaine, bCibleExterne est un booléen, sNomCible est une chaine  )
            
            	// Le clic sur le lien envoie vers un autre navigateur
            	si bCibleExterne ALORS
            	
            		// On traite selon le nom de la cible
            		selon sNomCible.Minuscule()
            			
            			// target="_blank"
            			cas cibleBlank
            								
            				// On ouvre l'URL dans un nouveau navigateur
            				OuvreNavigateurDifféré( sUrl, "" )
            				renvoyer vrai
            				
            			autres cas
            				
            				// On ouvre l'URL dans un nouveau navigateur avec alias
            				OuvreNavigateurDifféré( sUrl, sNomCible )
            				RENVOYER Vrai
            		FIN
            	FIN
            	
            
            	// Affiche l'URL dans le champ de saisie
            	SAI_BRW = sUrl
            	SAI_BRW.ExécuteTraitement( trtSortie )	
            	SAI_BRW.Etat	= Actif
            	
            	// Mémorise l'url
            	__URL = sUrl
            
            RENVOYER Vrai
