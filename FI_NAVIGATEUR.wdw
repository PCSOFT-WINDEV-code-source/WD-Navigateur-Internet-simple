  procedures :
   -
     name : DéclencheRecherche
     procedure_id : 2279945778054631377
     type_code : 14
     code : |1-
      // Résumé : Déclenche une recherche
      // Syntaxe :
      //DéclencheRecherche (<sRecherche> est chaîne)
      //
      // Paramètres :
      //	sRecherche (chaîne ANSI) : Mots clés de recherche
      // Valeur de retour :
      // 	Aucune
      //
      procédure DéclencheRecherche( sRecherche est une chaine )
      
      	// Recherche le moteur par défaut
      	soit N = MoteursRechercheDéfaut.Cherche( tcLinéaire, "ParDéfaut", vrai )
      	
      	// Aucun moteur par défaut, on prend le premier de la liste
      	si N < 1 alors N = 1
      		 
      	// Construit l'URL de recherche ( en UTF )
      	sURL est une chaine = MoteursRechercheDéfaut[ N ].URL.Remplace( "%s", sRecherche.VersUTF8().URLEncode( encodeURLDepuisUTF8 ) )
      	
      	// Change l'URL
      	FI_NAVIGATEUR.url = sURL
     type : 458752
   -
     name : NouvelleEntréeHistorique
     internal_properties : CAAAAAgAAAB1AXFvQ4H6X5WOe3BysglbGAKtepdFCIe1H53fHYHlsGJvKZZLqsFE1PDwWdempa4D7xXrHocMl8Jv0+h5dQuGLBAG7APwLlQUzbueTtJt1DLn8hZYKNnq+FXB6oF3UjDKVHUiWRM4xyUHZ2z5BR9H4gjkPLfm909m083Qbdb6eQ==
     procedure_id : 2279987817194246859
     type_code : 14
     code : |1-
      // Résumé : Mémorise une nouvelle entrée d'historique
      // Syntaxe :
      //NouvelleEntréeHistorique (<stEntrée> est EntréeHistorique)
      //
      // Paramètres :
      //	stEntrée (EntréeHistorique) : Entrée d'historique à mémoriser
      // Valeur de retour :
      // 	Aucune
      //
      procédure NouvelleEntréeHistorique( stEntrée est une EntréeHistorique )
      
      	// Ignore l'entrée si elle vient déjà de l'historique
      	si __origine = ( origineHistorique ) _ou_ stEntrée.URL = "" ALORS
      		retour
      	FIN
      
      	// Vérifie s'il s'agit d'une url système
      	si stEntrée.url pas [= "://" ALORS
      	
      		// Mémorise l'entrée dans l'historique général
      		Historique.Ajoute( stEntrée )		
      	
      		// Actualise le volet d'historique
      		si ChampExiste( "FI_HISTORIQUE" ) ALORS
      			FI_HISTORIQUE.ActualiseHistorique()
      		FIN
      	
      		// Mise à jour
      		HistoriqueSansDoublon <= Historique
      		HistoriqueSansDoublon.SupprimeDoublon( ttMembre, "URL" )
      	FIN
      	
      	// Si on n'a jamais fait 'back'
      	si __IndexHistorique = __HistoriqueLocal..Occurrence ALORS
      		
      		// Mémorise aussi dans l'historique local
      		__IndexHistorique = __HistoriqueLocal.Ajoute( stEntrée )
      		
      		SI __Origine <> OrigineOuverture ALORS			
      			BTN_PRECEDENT.grise = faux
      			BTN_SUIVANT.grise 	= vrai
      		FIN
      		
      	sinon
      		
      		// Tronque l'historique local jusqu'à l'index actuel
      		dimension( __HistoriqueLocal, __IndexHistorique )		
      		
      		// Mémorise aussi dans l'historique local
      		__IndexHistorique = __HistoriqueLocal.Ajoute( stEntrée )	
      		
      		SI __Origine <> OrigineOuverture ALORS
      			BTN_PRECEDENT.Grisé	= Faux
      			BTN_SUIVANT.Grisé	= Vrai
      		fin
      	FIN
     type : 458752
   -
     name : TraiteHistoriqueFavori
     internal_properties : CAAAAAgAAAA70H5bhIXKI8wAQbMsHj3hvu98z4qau7+P4bvBZ0cFwvLXzSLNZIsEYsqeS9FEgQTrn+nn3GUab+AZPUaPt/fkfdFF7RyLxctB+kYrpUPqYZvYiY/Bm0wFwZaA26hoZ8Vj/aR7op5T5l5BejaWhZAqKVd7vX8CA3z7zGzYAbe0tw==
     procedure_id : 2280011113097762270
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // TraiteHistoriqueFavori ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure TraiteHistoriqueFavori() < délai = 5ms >
      
      	// Relit l'url
      	sUrl est une chaine = FI_NAVIGATEUR.URL
      
      	// Crée un entrée pour mémoriser dans l'historique
      	stEntrée est une EntréeHistorique
      	stEntrée.Date	= DateHeureSys
      	stEntrée.Icône	= ImageVersDataURI( __Icône )
      	stEntrée.Titre	= FI_NAVIGATEUR.TitrePage
      	stEntrée.URL	= sUrl 
      	
      	// Mémorise l'entrée
      	NouvelleEntréeHistorique( stEntrée )
      	
      	// Teste l'url en favori
      	UrlTesteFavori( sUrl )
      	
      	BTN_FAVORIS_AJOUTE.Grisé	= Faux
      	OPT_FAVORI_AJOUTE.Grisé		= Faux
      
      	// Restaure l'origine du changement d'Url
      	__Origine = OrigineClic
     type : 458752
   -
     name : UrlTesteFavori
     internal_properties : CAAAAAgAAACzmnJz9wm6S3lhHoOMCH4hkMv9Ki3KcdBJGz0LQS054PbTJRKX3r3wZMSo9bPCwcbnYwlPChtYq3rbayyFue92hlrcRtXytMaub6n4YMC7gnw5WPjqqgdEOle/5DutoP48RmM0y31OlZCD0pm0QpP10IcNqx3J1U0sbe/mw/U8Pw==
     procedure_id : 2280225955951991527
     type_code : 14
     code : |1-
      // Résumé : Traite une url pour les favoris
      // Syntaxe :
      //UrlTesteFavori (<sUrl> est chaîne)
      //
      // Paramètres :
      //	sUrl (chaîne ANSI) : Url à tester
      // Valeur de retour :
      // 	Aucune
      //
      procédure UrlTesteFavori( sUrl est une chaine )
      
      	// Si la page est déjà dans les favoris
      	SI FavoriExiste( sUrl ) ALORS
      		
      		BTN_FAVORIS_AJOUTE.Image	= IMG_IS_FAVORITE
      		BTN_FAVORIS_AJOUTE.Bulle	= "Modifier le favori"
      		BTN_FAVORIS_AJOUTE.Note		= favoriEnregistré
      		
