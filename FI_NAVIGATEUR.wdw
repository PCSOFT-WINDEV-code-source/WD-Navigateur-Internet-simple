           code : |1-
            
            soit REGEXPURL = "((^(http)|^(HTTP))[sS]?(://)[a-zA-Z0-9\./%_\-]+[\?]?[a-zA-Z0-9\./\=&\+%_\-]*)|(^[a-zA-Z0-9\./%_\-]+\.[a-zA-Z0-9\./%_\-]*[a-zA-Z0-9\./%_\-]+[\?]?[a-zA-Z0-9\./\=&\+%_\-]*)"
            
            // Indique l'origine du changement d'Url
            __Origine = OrigineSaisie
            
            sUrl est une chaine = SAI_BRW
            
            // Elimine les espaces
            sUrl = sUrl.SansEspace()
            
            // Si la saisie correspond à une URL
            si sUrl.VérifieExpressionRégulière( REGEXPURL ) ALORS
            	
            	// On affecte l'URL
            	FI_NAVIGATEUR.url = sUrl
            	
            // C'est un fichier
            sinon si sUrl [~ "file:" _et_ fFichierExiste( fSéparateur( sUrl[ 9 a ], fSéparateurPlateforme ) )
            	
            	// On affecte le fichier
            	FI_NAVIGATEUR.URL = sUrl
            	
            // C'est une url système
            sinon si sUrl [= Prefixe_URL_Système alors 
            	
            	// Traite l'url
            	TraiteURLSystème( sUrl )
            		
            sinon
            	
            	// Déclenche une recherche
            	DéclencheRecherche( sUrl )
            FIN
