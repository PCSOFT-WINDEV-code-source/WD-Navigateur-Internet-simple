     name : FavoriAjouteOnglet
     procedure_id : 2280369364912985294
     type_code : 14
     code : |1+
      // Résumé : Ajoute l'onglet aux favoris
      // Syntaxe :
      // FavoriAjouteOnglet ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure FavoriAjouteOnglet()
      	
      	pstFavori	est une EntréeFavori dynamique
      	bAjout		est un booléen
      	sTitre		est une chaine	= FI_NAVIGATEUR.TitrePage
      	sUrl		est une chaine	= FI_NAVIGATEUR.url
      	
      	// Le site est déjà en favori
      	SI BTN_FAVORIS_AJOUTE.Note  <> favoriInconnu ALORS
      		
      		// Retrouve le favori
      		soit N = favoris.Cherche( tcLinéaire, "URL", sUrl )
      		
      		// Si on a retrouvé le favori
      		SI N > 0 ALORS
      			
      			// Garde une référence sur l'entrée
      			pstFavori = Favoris[ N ]
      		FIN
      		
      	FIN
      	
      	// Si on ne connaît toujours pas le favori
      	SI pstFavori = Null _OU_ BTN_FAVORIS_AJOUTE.Note = favoriInconnu ALORS
      		
      		// Crée une entrée
      		stFavori est un EntréeFavori
      		stFavori.Date	= DateHeureSys
      		stFavori.Nom	= sTitre
      		stFavori.URL	= sUrl
      		stFavori.Icône	= ImageVersDataURI( __Icône )
      		 
      		// Ajoute l'entrée
      		soit N = Favoris.Ajoute( stFavori )	
      		
      		// Garde une référence sur l'entrée
      		pstFavori	= Favoris[ N ]
      		
      		// On indique à la popup qu'on crée l'entrée
      		bAjout		= Vrai
      		
      	FIN
      	
      	// Ouvre la fenêtre d'ajout/modification des favoris
      	FEN_FAVORI.OuvrePopupPosition( poSelonChamp + poDroite, BTN_FAVORIS_AJOUTE, pstFavori, bAjout )
      	
      	// - Si le favori a été supprimé	
      	// - Si l'Url du favori a été modifiée
      	UrlTesteFavori( sUrl )
      	
      	// Sauve les favoris
      	SauveFavoris()
