      // Résumé : Modifie l'icône du volet associé au browser
      // Syntaxe :
      //ChangeIcôneVolet (<oFavicon> est Image)
      //
      // Paramètres :
      //	oFavicon (Image) : Image de l'icône
      // Valeur de retour :
      // 	Aucune
      //
      procédure ChangeIcôneVolet( local oFavicon est une image )
      
      	// Redimensionne l'image
      	oFavicon.Redimensionne( 16, 16, drHauteQualité + drHomothétiqueCentré )
      	
      	// Crée une image un peu plus large pour laisser une marge entre l'icône et le titre
      	oVoletImage est une image
      	oVoletImage.Largeur	= oFavicon.Largeur + 8
      	oVoletImage.Hauteur	= oFavicon.Hauteur
      	
      	// Copie le favicon dans l'image du volet
      	oFavicon.Copie( oVoletImage, 0, 0, oFavicon.Largeur, oFavicon.Hauteur, 0, 0, oFavicon.Largeur, oFavicon.Hauteur )
      	
      	// Affecte l'image au volet d'onglet
      	__Volet.Image = oVoletImage
