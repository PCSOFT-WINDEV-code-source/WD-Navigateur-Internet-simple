     name : TraiteURLSystème
     internal_properties : CAAAAAgAAAAqt2w0502c9TwfqR4AYlzWQM6fQwD2DXDwLdkJzrf3OP1b3iuqi5kcfJE0IuiqcsCxWvIh2xTDmOILLFrkkPHYZaRGggOhNs14YJOMHGPfCezoIHeavtSy6GJz+x02mAm8Kz4iK24DvFihRqYhN1zztVWl6v+Ul3ovNCV01D9hQQPzuQsdH/Tm02Kw79cCHtUno7+v1WiSM+Lku9297T49HqgzoPbNrvF3zLYt
     procedure_id : 2280276731054144393
     type_code : 14
     code : |1-
      // Résumé : Traite une Url système
      // Syntaxe :
      //TraiteURLSystème (<sUrl> est chaîne)
      //
      // Paramètres :
      //	sUrl (chaîne ANSI) : Url à traiter
      // Valeur de retour :
      // 	Aucune
      //
      procédure TraiteURLSystème( sUrl est une chaine )
      
      	// Si c'est une url système
      	// Vérifie si on n'a pas déjà un volet ouvert avec cette url
      	si sUrl [= Prefixe_URL_Système ALORS
      		
      		// Récupère le champ onglet du volet
      		oOnglet est un Champ <- { OngletDuChamp( mafenetreinterne ), indChamp }
      				
      		// Enumère les volets de l'onglet
      		soit N = 1 
      		soit Alias = EnumèreChamp( oOnglet, N )
      		
      		TANTQUE Alias <> ""
      			
      			// Prend une référence sur le navigateur ouvert dans le volet
      			oNavigateur est un Champ <- oOnglet[ Alias ].FI_NAVIGATEUR
      			
      			// Vérifie l'URL
      			si oNavigateur.__URL = sUrl ALORS
      				
      				// Force le volet à être affiché
      				oOnglet			= Alias
      				RETOUR			
      			FIN			
      				
      			// Volet suivant
      			N ++
      			Alias = EnumèreChamp( oOnglet, N )
      		FIN
      	FIN
      
      	// Synchronise le champ de saisie
      	__URL	= sUrl
      	SAI_BRW = sUrl
      	SAI_BRW.ExécuteTraitement( trtSortie )
      
      	SI sUrl ~= URL_Paramètres
      	
      		// Ouvre les paramètres
      		AfficheParamètres()
      				
      	SINON SI sUrl ~= URL_Favoris
      		
      		// Ouvre les favoris
      		AfficheFavoris()
      		
      	SINON SI sUrl ~= URL_Historique
      	
      		// Ouvre l'historique
      		AfficheHistorique()
      		
      	SINON SI sUrl ~= URL_Accueil
      	
      		// Ouvre l'accueil
      		AfficheAccueil()
      				
      	fin
     type : 458752
