           internal_properties : CAAAAAgAAAAeAd8I/SDyqU0gk5Cn/iy7cWfxVByjmUAE78SMJmRSUZZCXryTRZvTV8D9+k9wQloJtiiAuUL0sRhD/10GQhxV0H7Up8uBlaOD5ZwOM4TFrmQOdGVLaf0Q+nvqfnF/yLMHydGhOWwWhNyCo82KyN2yVxTdOthEIuNipFPsxL0Yj48qhxhV0wTFLk8XoHPlc8ta
           procedure_id : 2279965891385421343
           type_code : 14
           code : |1-
            // Résumé : Récupère le titre de la page actuellement affichée
            // Syntaxe :
            //RécupèreTitre ()
            //
            // Paramètres :
            //	Aucun
            // Valeur de retour :
            // 	Aucune
            //
            procédure RécupèreTitre() 
            	
            	// Exécute le code JS
            	HTM_BROWSER.ExécuteJS("document.location.href + '\t' + document.title;", __SurExécuteJS )
            	
            	procedure interne __SurExécuteJS( bRes est un booléen, sValeurRetour est une chaine )
            		
            		// Si l'exécution a réussi
            		si bRes ALORS
            			
            			// Récupère l'url
            			sUrl est une chaine = sValeurRetour.ExtraitChaîne( 1 )
            			
            			// Affiche l'URL dans le champ de saisie
            			SAI_BRW = sUrl
            			SAI_BRW.ExécuteTraitement( trtSortie )	
            			SAI_BRW.Etat	= Actif
            			
            			// Mémorise l'url
            			__URL = sUrl
            			
            			FI_NAVIGATEUR.TitrePage = sValeurRetour.Extraitchaine( 2 )
            						
            			// Récupère l'icône du site
            			RécupèreIcône()
            			
            			// Traite l'historique et les favoris
            			TraiteHistoriqueFavori()
            		FIN
            	FIN
           type : 458752
