      -
        internal_properties : 
        code : |1-
         PROCEDURE MaFenêtre( gsURL est une chaine = "", gsAlias est une chaine = "" )
         
         OrigineUrl est une énumération
         	OrigineClic					// Un clic déclenche le changement d'url
         	OrigineHistorique			// On change d'url depuis l'historique
         	OrigineSaisie				// On change d'url depuis une saisie dans la barre d'adresse
         	OrigineOuverture			// On applique la première url, lors de l'ouverture du volet
         fin
         
         // Urls système
         constant
         	Prefixe_URL_Système	= "://"
         	URL_Favoris			= Prefixe_URL_Système + "bookmarks"<§aad:0§>
         	URL_Paramètres		= Prefixe_URL_Système + "settings"
         	URL_Historique		= Prefixe_URL_Système + "history"
         	URL_Accueil			= Prefixe_URL_Système + "home"
         fin
         
         __Icône				est une image						// Icône du site affiché ("favicon")
         __AliasWeb			est une chaine						// Alias du volet de navigateur
         __URL				est une chaine						// URL en cours
         __Titre				est une chaine = "Nouvel onglet"	// Titre du volet de navigateur
         __Volet				est un champ						// Volet associé à la fenêtre interne
         __HistoriqueLocal	est un tableau d'EntréeHistorique	// Historique de navigation local à la fenêtre interne
         __IndexHistorique	est un entier						// Index dans l'historique de navigation local
         __Origine			est une OrigineUrl					// Origine du changement d'url
         
         // Affichage initial
         BTN_ACTUALISER.plan		= 0
         BTN_BRW_RECHERCHE.grise	= vrai
         BTN_ACTUALISER.grise	= vrai
         BTN_PRECEDENT.grise		= vrai
         BTN_SUIVANT.grise		= vrai
      -
        code : |1-
         
         // Mémorise le volet associé
         __Volet <- VoletDuChamp( mafenetreinterne )
         
         // Change l'origine du changement d'Url
         __Origine = OrigineOuverture
         
         // Navigue vers l'URL
         FI_NAVIGATEUR.AliasWeb 	= gsAlias
         FI_NAVIGATEUR.URL 		= gsURL
         
         // Si on a ouvert un volet vierge
         si gsURL = "" ALORS
         	
         	TraiteURLSystème( URL_Accueil )
         	
         	// Rend le champ de saisie actif et active la saisie 
         	SAI_BRW.etat = actif
         	ForceSaisie()
         	
         	procedure interne ForceSaisie() < délai = 5ms >
         		
         		// Aucune URL saisie
         		BTN_BRW_RECHERCHE.Plan	= 0
         		IMG_CHARGEMENT.Plan		= 1
         		donnefocus( SAI_BRW )
         	fin
         FIN
        type : 14
      -
        code : |1-
         
         // Sauvegarde l'historique
         SauveHistorique()
