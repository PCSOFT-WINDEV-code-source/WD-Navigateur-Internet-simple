           internal_properties : CAAAAAgAAACy5VqKsnRqkEkMRVmHXS0rqTse8fwP6gbzbHJ4XuL6ClvAfhnHm8UmSz8h41GMyKY8LoOPpGOUX6gXZvrSU7/JanSaJEgJShk1UZuKYFyEhB+VJBg1ldfrR70FusGjkchnpB8cPqwicDwVQmAafU9W3LMxU1DDJTEiNgQVoi8rSaSi
           procedure_id : 2279965861320578219
           type_code : 14
           code : |1-
            // Résumé : <indiquez ici ce que fait la procédure>
            // Syntaxe :
            // RécupèreIcône ()
            //
            // Paramètres :
            //	Aucun
            // Valeur de retour :
            // 	Aucune
            //
            procédure RécupèreIcône()
            
            	// Exécute le code JS
            	HTM_BROWSER.ExécuteJS([
            	var e = document.querySelector('link[rel$="shortcut icon"]');
            	if ( e != null )
            		return e.href;	
            	return '';
            	], __SurExécuteJS )
            	
            		PROCÉDURE INTERNE __SurExécuteJS( bRes est un booléen, sValeurRetour est une chaine )
            
            			sFavicon est une chaine = ""
            		
            			// Si l'exécution a réussi
            			SI bRes ALORS
            								
            				// Si on n'a pas de valeur
            				si sValeurRetour <> "" ALORS
            					
            					// Mémorise la la valeur comme chemin d'icône
            					sFavicon = sValeurRetour	
            				fin
            			fin
            				
            			si sFavicon = "" ALORS
            				
            				// Construit une URL d'accès au 'favicon.ico'
            				sFavicon = URLExtraitChemin( FI_NAVIGATEUR.URL, urlProtocole + urlDomaine + urlPort + urlUtilisateur + urlMotDePasse ) + [ "/" ] + "favicon.ico"									
            			FIN
            			
            			TéléchargementFavicon:
            				
            			// Prépare le téléchargement de l'icône
            			stFavicon est une httpRequête
            			stFavicon.url = sFavicon
            			stFavicon.IgnoreErreur = httpIgnoreRenvoiHTTPS + httpIgnoreRenvoiHTTP + httpIgnoreRedirection + httpIgnoreCertificatExpiré + httpIgnoreCertificatInvalide + httpIgnoreNomCertificatInvalide
            				
            			// Télécharge l'icône
            			stRéponse est une httpRéponse = stFavicon.Envoie()
            			
            			// On a obtenu une redirection ?
            			si stRéponse.CodeEtat dans ( 301, 302 ) ALORS
            				
            				// Récupère la nouvelle URL
            				sFavicon = stRéponse.entete[ "Location" ]
            				
            				// Recommence le téléchargement
            				goto TéléchargementFavicon
            			FIN						
            			
            			// On a obtenu une bonne réponse
            			si stRéponse.codeetat = 200 ALORS
            				
            				// Charge l'icône
            				__Icône = stRéponse..Contenu
            				
            				// Change l'icône du volet
            				ChangeIcôneVolet( __Icône )				
            			FIN
            		FIN
