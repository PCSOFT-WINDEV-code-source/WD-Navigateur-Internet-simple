     name : ConstruitMenuHistorique
     internal_properties : CAAAAAgAAACD3WN0N+kVV0Qc227QhtnNa0cVqgvnbsyi89OjYo3/6Y39ygldcrud+thZLJxO2SqOLMbRD5KtDnS4RIRES2hfJ6IQadKBHFncBylEiuImG5huEatc5ODQxMs3knyX6aB3UVJ9CletRLhQaD7fsBS5Zf6MWAcxRxtuvH6L2qkfVuGgRgbWZhOFAn2UYnq/DHIWEg==
     procedure_id : 2281363770109207967
     type_code : 14
     code : |1-
      // Résumé : Construit le menu d'historique
      // Syntaxe :
      // ConstruitMenuHistorique ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure ConstruitMenuHistorique()
      
      	X est un entier
      				
      	// Récupère l'historique mais supprime les doublons d'url
      	tabHistorique est un tableau d'EntréeHistorique <= historique
      	tabHistorique.SupprimeDoublon( ttMembre, "URL" )
      	
      	MENU_PARAMETRES.OPT_HISTORIQUE.OPT_Séparateur5.Visible = ( tabHistorique.vide = faux )
      	
      	// On autorise maximum 10 entrées d'historique
      	pour i = 1 _a_ 10
      		
      		X ++
      		oOption est un champ <- { "OPT_HISTORIQUE_ENTREE_" + i, indChamp }
      		
      		si X > tabHistorique..Occurrence ALORS
      			oOption.Visible = faux
      			continuer
      		FIN
      		
      		// Prend une référence sur l'entrée d'historique
      		stEntrée est une EntréeHistorique = tabHistorique[ X ]
      		
      		// Si l'url de l'entrée est la même que celle affichée dans le navigateur
      		tantque stEntrée.url = FI_NAVIGATEUR.url
      			
      			// on cherche l'entrée suivante
      			X ++
      			
      			// Si il n'y a plus d'entrée d'historique disponible
      			SI X > tabHistorique..Occurrence ALORS
      				
      				//  Cache l'option et on reprend le parcours des options
      				oOption.Visible = Faux
      				CONTINUER(2)
      			FIN 
      			
      			stEntrée = tabHistorique[ X ]			
      		FIN
      		
      		// Modifie l'option
      		oOption.Visible = vrai
      		oOption.grise 	= faux
      		oOption.Libellé = stEntrée.Titre
      		oOption.Image	= ( DataURIVersImage( stEntrée.Icône, 16, 16 ) )
      		
      		// Modifie le traitement de clic sur l'option de menu : on génère l'appel à une procédure
      		oOption.Traitement[ trtOptionMenu ] = __OuvreUrl( stEntrée.Url )			
      	FIN
      	
      	procedure interne __OuvreUrl( local sUrl est une chaine )
      		
      		// Crée une procédure qui ouvrira le volet avec l'url
      		pOuvreNav est une procedure = __OuvreUrlOptionMenu
      		
      		procedure interne __OuvreUrlOptionMenu()
      			// Ouvre un onglet avec l'URL
      			OuvreNavigateur( sUrl, "", Faux )
      		fin
      		
      		renvoyer pOuvreNav
      	FIN
     type : 458752
