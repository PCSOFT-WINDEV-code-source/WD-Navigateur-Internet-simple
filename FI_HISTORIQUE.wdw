  procedures :
   -
     name : ActualiseHistorique
     internal_properties : CAAAAAgAAACerzG795JULfb2t7Af9dv2OOUqZVoHuVA1z1rEuBWgD77X7bGn4ykYOWqz4cicrsmk/zEQCtwfIr0tkmwpgL8yV43FalrYjYx1dtTXbSBlarHOjZRXWu0ubI67LG34hUzRLrzvlNFU/1DSuiFF
     procedure_id : 2280330997967816559
     type_code : 14
     code : |1-
      // Résumé : Actualise les favoris
      // Syntaxe :
      //ActualiseHistorique ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure ActualiseHistorique()
      	
      	dhMaintenant est une dateheure = dateheuresys
      	
      	// Vide la liste
      	ZR_HISTORIQUE.SupprimeTout()
      	ZR_HISTORIQUE.DernièreLigne = 0
      		
      	pour tout stEntrée de historique sur Date depuisfin
      			
      		// Ajoute l'entrée d'historique
      		soit N = ZR_HISTORIQUE.Ajouteligne( stEntrée.Date..PartieDate, stEntrée.Date..PartieHeure, stEntrée.URL )
      			
      		sDurée est une chaine
      		
      		// Aujourd'hui
      		si dhMaintenant..partiedate = stEntrée.Date..partiedate ALORS
      			sDurée = "Aujourd'hui"
      		sinon
      			
      			dhHier est une dateheure = dhMaintenant - 1 j
      			dhAvantHier est une dateheure = dhHier - 1 j
      			
      			// Hier ?
      			si dhHier..PartieDate = stEntrée.Date..partiedate ALORS
      				sDurée = "Hier"
      				
      			// Avant-hier ?
      			sinon si dhAvantHier..partiedate = stEntrée.Date..partiedate ALORS
      				sDurée = "Avant-hier"
      			
      			// Autres jours
      			sinon
      				sDurée = DateHeureVersChaîne( stEntrée.Date..PartieDate, maskDateDuréeRelative )
      			FIN
      		FIN
      			
      		// Actualise les données affichées
      		ZR_HISTORIQUE[ N ].LIB_DATE		= sDurée + " - " + DateVersChaîne( stEntrée.Date..partiedate, "jjjj JJ mmmm AAAA" )
      		ZR_HISTORIQUE[ N ].IMG_FAVICON	= DataURIVersImage( stEntrée.Icône )
      		ZR_HISTORIQUE[ N ].LIB_ENTREE	= gCadrage( "GV", gPoliceGras( Vrai ) + stEntrée.Titre + gPoliceGras( Faux ) + TAB + gStylo( GrisFoncé ) + URLExtraitChemin( stEntrée.URL, urlDomaine ) )
      		
      	FIN
      
      	BTN_TOUT_SUPPR.Visible	 = pas ( Historique.Vide  )
      	ZR_HISTORIQUE.Visible    = pas ( Historique.Vide )
      	LIB_LIEN.Visible    	 = ( Historique.Vide )
      
      	// Affiche les infos de sélection
      	NbSélections = 0
      	AfficheInfoSélection()
     type : 458752
   -
     name : AfficheInfoSélection
     procedure_id : 2287040570112441576
     type_code : 14
     code : |1-
      // Résumé : Affiche les infos de sélection
      // Syntaxe :
      // AfficheInfoSélection ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure AfficheInfoSélection()
      
      	// Marque du pluriel
      	s est une chaîne = ( NbSélections > 1 ? "s" SINON "" )
      	
      	LIB_NO_SELECTION.Visible	= ( NbSélections > 0  )
      	BTN_SELECT_SUPPR.Visible	= ( NbSélections > 0  )
      	LIB_NO_SELECTION			= NbSélections + " élément[%s%] sélectionné[%s%]"
     type : 458752
