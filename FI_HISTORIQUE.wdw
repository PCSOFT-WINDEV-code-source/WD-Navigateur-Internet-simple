              code : |1-
               
               // Multisélection ?
               si ToucheEnfoncée( teShift ) _et_ DernièreLigne > 0 ALORS
               	
               	ZRMin, ZRMax sont des entiers 
               	
               	si ZR_HISTORIQUE > DernièreLigne ALORS
               		ZRMin = DernièreLigne
               		ZRMax = ZR_HISTORIQUE
               	sinon
               		ZRMin = ZR_HISTORIQUE		
               		ZRMax = DernièreLigne
               	fin
               		
               	pour N = ZRMin _a_ ZRMax
               		
               		// Si l'interrupteur est différent
               		si ZR_HISTORIQUE[ N ].INT_SELECT <> MoiMême _ou_ N = ZR_HISTORIQUE ALORS
               			
               			// Change l'interrupteur
               			ZR_HISTORIQUE[ N ].INT_SELECT = MoiMême
               			
               			SI MoiMême ALORS
               				NbSélections ++
               			SINON
               				NbSélections --
               			FIN
               			
               		FIN
               	FIN
               sinon
               	
               	si moimeme ALORS
               		NbSélections ++
               	sinon
               		NbSélections --
               	FIN
               	
               FIN
               
               // Affiche les infos de sélection
               AfficheInfoSélection()
               
               DernièreLigne = ZR_HISTORIQUE
